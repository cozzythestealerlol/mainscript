-- ----------------------------
-- Base Data
-- ----------------------------
local running = false
local currentTween

local predefinedBases = {
    {"Base 1", Vector3.new(-80.16,10.12,208.80), Vector3.new(-75.44,11.40,265.99)},
    {"Base 2", Vector3.new(9.71,10.27,209.32), Vector3.new(15.17,11.42,264.96)},
    {"Base 3", Vector3.new(99.67,10.00,207.17), Vector3.new(105.93,11.42,266.59)},
    {"Base 4", Vector3.new(201.57,10.06,214.68), Vector3.new(207.85,11.36,270.71)},
    {"Base 5", Vector3.new(10.59,10.12,72.01), Vector3.new(5.43,11.43,15.66)},
    {"Base 6", Vector3.new(100.11,10.06,73.35), Vector3.new(96.19,11.42,14.93)},
    {"Base 7", Vector3.new(190.03,10.27,71.58), Vector3.new(185.11,11.42,13.97)},
    {"Base 8", Vector3.new(-79.26,10.12,71.99), Vector3.new(-85.57,11.43,15.35)}
}

local basePositions = {predefinedBases[7][2], predefinedBases[7][3]}
local baseName = "Base 7"

-- Auto detect nearest base
local function detectNearestBase()
	local nearestDist = math.huge
	local nearestBase = nil
	if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
		local hrp = player.Character.HumanoidRootPart
		for _, b in ipairs(predefinedBases) do
			local dist = (hrp.Position - b[2]).Magnitude
			if dist < nearestDist then
				nearestDist = dist
				nearestBase = b
			end
		end
	end
	if nearestBase then
		baseName = nearestBase[1]
		basePositions = {nearestBase[2], nearestBase[3]}
		selectedBaseLabel.Text = "Selected Base: "..baseName
	end
end

detectNearestBase()

-- ----------------------------
-- Choose Base GUI
-- ----------------------------
chooseBaseBtn.MouseButton1Click:Connect(function()
	local baseGui = Instance.new("ScreenGui")
	baseGui.Name = "BaseSelectorGui"
	baseGui.Parent = player:WaitForChild("PlayerGui")

	local baseFrame = Instance.new("Frame")
	baseFrame.Parent = baseGui
	baseFrame.Size = UDim2.new(0,300,0,450)
	baseFrame.Position = UDim2.new(0.5,0,0.5,0)
	baseFrame.AnchorPoint = Vector2.new(0.5,0.5)
	baseFrame.BackgroundColor3 = Color3.fromRGB(50,50,50)
	baseFrame.BackgroundTransparency = 0.55
	baseFrame.BorderSizePixel = 0
	local corner = Instance.new("UICorner", baseFrame)
	corner.CornerRadius = UDim.new(0,20)

	local title = Instance.new("TextLabel")
	title.Parent = baseFrame
	title.Text = "Select Base"
	title.Size = UDim2.new(1,0,0,40)
	title.Position = UDim2.new(0,0,0,10)
	title.TextColor3 = Color3.fromRGB(255,255,255)
	title.Font = Enum.Font.GothamBold
	title.TextSize = 24
	title.BackgroundTransparency = 1

	for i, info in ipairs(predefinedBases) do
		local btn = Instance.new("TextButton")
		btn.Parent = baseFrame
		btn.Size = UDim2.new(0,200,0,40)
		btn.Position = UDim2.new(0.5,-100,0,60+(i-1)*50)
		btn.BackgroundColor3 = Color3.fromRGB(80,80,80)
		btn.TextColor3 = Color3.fromRGB(255,255,255)
		btn.Font = Enum.Font.GothamBold
		btn.TextSize = 20
		btn.Text = info[1]
		local corner = Instance.new("UICorner", btn)
		corner.CornerRadius = UDim.new(0,12)

		btn.MouseButton1Click:Connect(function()
			basePositions = {info[2], info[3]}
			baseName = info[1]
			selectedBaseLabel.Text = "Selected Base: "..baseName
			baseGui:Destroy()
			if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
				-- TP instantly to first coordinate
				player.Character.HumanoidRootPart.CFrame = CFrame.new(info[2])
			end
		end)
	end
end)

-- ----------------------------
-- Stealing Util
-- ----------------------------
actionBtn.MouseButton1Click:Connect(function()
	if not running then
		running = true
		actionBtn.Text = "Stop Stealing Util"
		statusLabel.Text = "Active"
		coroutine.wrap(function()
			local character = player.Character
			if character and character:FindFirstChild("HumanoidRootPart") then
				local hrp = character.HumanoidRootPart

				-- Teleport to special coordinates
				hrp.CFrame = CFrame.new(432.40,10,96.18)

				wait(4) -- wait for interaction

				-- Tween to first coordinate of base
				local firstTween = TweenService:Create(hrp, TweenInfo.new(1,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),{CFrame=CFrame.new(basePositions[1])})
				firstTween:Play()
				firstTween.Completed:Wait()

				-- Tween to second coordinate of base
				local secondTween = TweenService:Create(hrp, TweenInfo.new(1,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),{CFrame=CFrame.new(basePositions[2])})
				secondTween:Play()
				secondTween.Completed:Wait()
			end
			running = false
			actionBtn.Text = "Stealing Util"
			statusLabel.Text = "Ready To Use!"
		end)()
	else
		running = false
		if currentTween then
			currentTween:Cancel()
			currentTween = nil
		end
		actionBtn.Text = "Stealing Util"
		statusLabel.Text = "Ready To Use!"
	end
end)

-- ----------------------------
-- Instant ProximityPrompt
-- ----------------------------
local function setStealPrompt()
	for _, descendant in ipairs(Workspace:GetDescendants()) do
		if descendant:IsA("ProximityPrompt") and descendant.Name == "StealPrompt" then
			if descendant.HoldDuration ~= 0 then
				descendant.HoldDuration = 0
			end
		end
	end
end

