title.Text = "Tycoon Item Finder"

-- Scrollable list
local scrollingFrame = Instance.new("ScrollingFrame")
scrollingFrame.Size = UDim2.new(1, -20, 1, -80)
scrollingFrame.Position = UDim2.new(0, 10, 0, 60)
scrollingFrame.BackgroundTransparency = 1
scrollingFrame.ScrollBarThickness = 6
scrollingFrame.CanvasSize = UDim2.new(0,0,0,0)
scrollingFrame.Parent = frame

local uiListLayout = Instance.new("UIListLayout")
uiListLayout.Parent = scrollingFrame
uiListLayout.SortOrder = Enum.SortOrder.LayoutOrder
uiListLayout.Padding = UDim.new(0,5)

local uiPadding = Instance.new("UIPadding")
uiPadding.Parent = scrollingFrame
uiPadding.PaddingTop = UDim.new(0,5)
uiPadding.PaddingBottom = UDim.new(0,5)
uiPadding.PaddingLeft = UDim.new(0,5)
uiPadding.PaddingRight = UDim.new(0,5)

-- Function to update CanvasSize
local function updateCanvas()
    scrollingFrame.CanvasSize = UDim2.new(0, 0, 0, uiListLayout.AbsoluteContentSize.Y + 5)
end

-- Scan Tycoons and populate GUI
local function scanTycoons()
    -- Clear previous buttons
    for _, child in ipairs(scrollingFrame:GetChildren()) do
        if child:IsA("TextButton") then
            child:Destroy()
        end
    end

    local map = workspace:FindFirstChild("Map")
    if not map then return end
    local tycoonsFolder = map:FindFirstChild("Tycoons")
    if not tycoonsFolder then return end

    local itemsToDisplay = {}

    -- Collect all items first
    for i = 1, 8 do
        local tycoonName = "Tycoon"..i
        local tycoon = tycoonsFolder:FindFirstChild(tycoonName)
        if tycoon then
            local tempFolder = tycoon:FindFirstChild("Temporary")
            if tempFolder then
                for _, item in ipairs(tempFolder:GetChildren()) do
                    if item:IsA("BasePart") or item:IsA("Model") then
                        local rarityText = "Unknown"
                        local trashInfo = item:FindFirstChild("TrashInfo")
                        if trashInfo then
                            local rarityLabel = trashInfo:FindFirstChild("Rarity")
                            if rarityLabel and rarityLabel:IsA("TextLabel") then
                                rarityText = rarityLabel.Text
                            end
                        end
                        table.insert(itemsToDisplay, {item = item, name = item.Name, rarity = rarityText})
                    end
                end
            end
        end
    end

    -- Sort items by exact rarity order
    table.sort(itemsToDisplay, function(a, b)
        return getRarityRank(a.rarity) < getRarityRank(b.rarity)
    end)

    -- Create buttons
    for _, info in ipairs(itemsToDisplay) do
        local item = info.item
        local itemName = info.name
        local rarityText = info.rarity

        local btn = Instance.new("TextButton")
        btn.Parent = scrollingFrame
        btn.Size = UDim2.new(1, -10, 0, 50)
        btn.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
        btn.BackgroundTransparency = 0.5
        btn.BorderSizePixel = 0
        btn.Text = ""  -- remove default text
        local btnCorner = Instance.new("UICorner", btn)
        btnCorner.CornerRadius = UDim.new(0,10)
        btn.LayoutOrder = #scrollingFrame:GetChildren()

        -- Name label
        local nameLabel = Instance.new("TextLabel")
        nameLabel.Parent = btn
        nameLabel.Size = UDim2.new(1, -10, 0.5, 0)
        nameLabel.Position = UDim2.new(0,5,0,0)
        nameLabel.BackgroundTransparency = 1
        nameLabel.Text = itemName
        nameLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        nameLabel.Font = Enum.Font.GothamBold
        nameLabel.TextSize = 18
        nameLabel.TextXAlignment = Enum.TextXAlignment.Left

        -- Rarity label with color
        local rarityLabel = Instance.new("TextLabel")
        rarityLabel.Parent = btn
        rarityLabel.Size = UDim2.new(1, -10, 0.5, 0)
        rarityLabel.Position = UDim2.new(0,5,0.5,0)
        rarityLabel.BackgroundTransparency = 1
        rarityLabel.Text = rarityText
        rarityLabel.Font = Enum.Font.Gotham
        rarityLabel.TextSize = 16
        rarityLabel.TextXAlignment = Enum.TextXAlignment.Left

        -- Apply color based on rarity
        local cleanRarity = trim(rarityText)
        rarityLabel.TextColor3 = rarityColors[cleanRarity] or Color3.fromRGB(200,200,255)

        -- Teleport on click
        btn.MouseButton1Click:Connect(function()
            if item.PrimaryPart then
                player.Character:SetPrimaryPartCFrame(item.PrimaryPart.CFrame + Vector3.new(0,5,0))
            elseif item:IsA("BasePart") then
                player.Character:SetPrimaryPartCFrame(item.CFrame + Vector3.new(0,5,0))
            end
        end)
    end

    updateCanvas()
end

-- Initial scan
scanTycoons()

-- Auto-update every 5 seconds
while true do
    task.wait(5)
    scanTycoons()
end
	print("Brainrot Finder button clicked!")
end)

-- ----------------------------
-- Auto-update nearest base
-- ----------------------------
detectNearestBase()
