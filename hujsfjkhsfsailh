		descendant.HoldDuration = 0
	end
end)

-- ----------------------------
-- Brainrot Finder
-- ----------------------------
brainrotBtn.MouseButton1Click:Connect(function()
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local workspace = game:GetService("Workspace")
local Lighting = game:GetService("Lighting")
local RunService = game:GetService("RunService")

-- Exact rarity order (top to bottom)
local rarityOrder = {
    "Limited",
    "Admin",
    "Secret",
    "Wanted",
    "Dangerous",
    "Mythical",
    "Legendary",
    "Epic",
    "Rare",
    "Uncommon",
    "Common"
}

-- Rarity color mapping
local rarityColors = {
    ["Limited"] = Color3.fromRGB(255, 192, 203),    -- Pink
    ["Admin"] = Color3.fromRGB(255, 0, 0),          -- Red
    ["Secret"] = Color3.fromRGB(128, 128, 128),     -- Grey
    ["Wanted"] = Color3.fromRGB(128, 0, 128),       -- Purple
    ["Dangerous"] = Color3.fromRGB(255, 0, 0),      -- Red again
    ["Mythical"] = Color3.fromRGB(255, 165, 0),     -- Orange
    ["Legendary"] = Color3.fromRGB(255, 255, 0),    -- Yellow
    ["Epic"] = Color3.fromRGB(128, 0, 128),         -- Purple again
    ["Rare"] = Color3.fromRGB(0, 0, 255),           -- Blue
    ["Uncommon"] = Color3.fromRGB(0, 255, 0),       -- Green
    ["Common"] = Color3.fromRGB(128, 128, 128)      -- Grey again
}

-- Helper: trim string
local function trim(s)
    return s:gsub("^%s*(.-)%s*$", "%1")
end

-- Get rank for sorting
local function getRarityRank(rarity)
    rarity = trim(rarity)
    for i, v in ipairs(rarityOrder) do
        if v == rarity then
            return i
        end
    end
    return #rarityOrder + 1 -- unknown rarities go at bottom
end

-- GUI Setup
local gui = Instance.new("ScreenGui")
gui.Name = "TycoonItemFinderGui"
gui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
gui.IgnoreGuiInset = true
gui.ResetOnSpawn = false
gui.Parent = player:WaitForChild("PlayerGui")

-- Blur Effect
local blur = Instance.new("BlurEffect")
blur.Size = 0
blur.Parent = Lighting

-- Background frame with gradient
local bgFrame = Instance.new("Frame")
bgFrame.Parent = gui
bgFrame.Size = UDim2.new(1, 0, 1, 0)
bgFrame.BackgroundTransparency = 1
local bgGradient = Instance.new("UIGradient", bgFrame)
bgGradient.Color = ColorSequence.new{
    ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 160, 130)),
    ColorSequenceKeypoint.new(0.5, Color3.fromRGB(100, 150, 255)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(255, 220, 200))
}
bgGradient.Rotation = 45

-- Main frame
local frame = Instance.new("Frame")
frame.Parent = gui
frame.Size = UDim2.new(0, 450, 0, 550)
frame.Position = UDim2.new(0.5, 0, 0.5, 0)
